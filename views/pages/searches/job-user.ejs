<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <% console.log(data1) %>
    <% data1.forEach(item=>{ %>.
    <img src="<%= item.logo %>" alt="" width="50px">
    <h3><%= item.title %></h3>
    <p><%= item.type %></p>
    <p><%= item.company_name %></p>
    <p><%= item.country %></p>
    <button>APPLY</button>
    <div>
        <fieldset>
            <form action="/search/job/apply/<%=item.id%>" method="POST">
                <img src="<%= item.logo %>" alt="" width="50px">
                <p><%= item.company_name %></p>
                <p>Job Application</p>

                <img src="<%= data3.avatar%>" alt="" width="50px">
                <input type="text" name="first_name" value="<%= data3.first_name%>">
                <input type="text" name="last_name" value="<%= data3.last_name%>">
                <input type="text" name="country" value="<%= data3.country%>">
                <input type="text" name="phone" value="<%=data3.phone%>">
                <input type="hidden" name="company_id" value="<%= item.company_id %>">
                <input type="hidden" name="personID" value="<%= data3.id %>">
                <a href="<%= data3.cv%>"></a>
                <input type="hidden" name="cv" value="<%= data3.cv%>">
                <input type="submit" value="APPLY My Application">
            </form>
        </fieldset>

    </div>

    <%})%>

