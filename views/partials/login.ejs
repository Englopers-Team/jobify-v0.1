<div id="js-lightbox-bg" class="res-login-popup-bg"></div>
<div class="res-login-popup" id="js-lightbox-main">
	<div class="res-login js-lightbox-login">
		<form id="loginForm" action="/login" method="post">
			<h3 id="logoo">Log In</h3>
			<label for="email">Email</label>
			<input type="email" id="email" name="email" value="" placeholder="Type in your email.." autocomplete="off"
				required />

			<label for="password">Password</label>
			<input type="password" id="password" name="password" value="" placeholder="Enter your password.."
				autocomplete="off" required />
			<input type="hidden" class="inputSession" name="session_id" value="">
			<a class="register" href="/signup">Sign up</a>
			<input type="submit" name="submit" value="Log In" />
		</form>
	</div>
</div>